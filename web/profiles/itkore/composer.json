{
  "name": "drupal/itkore",
  "description": "ITKore install profile",
  "type": "drupal-profile",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "~1.0",
    "drupal/token": "~1",
    "drupal/imce": "~1",
    "drupal/ctools": "~3",
    "drupal/pathauto": "~1",
    "drupal/field_group": "~1",
    "drupal/youtube": "~1",
    "drupal/entity_reference_revisions": "~1",
    "drupal/paragraphs": "~1",
    "drupal/adminimal_theme": "~1",
    "drupal/metatag": "~1",
    "drupal/redirect": "~1",
    "drupal/role_delegation": "~1",
    "drupal/toolbar_visibility": "~1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "sites/all/modules/contrib/{$name}/": ["type:drupal-module"]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ]
}
